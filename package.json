{
  "name": "erp-be",
  "version": "1.0.0",
  "description": "erp-be",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server",
    "migrate": "sequelize db:migrate",
    "undo-migrate": "sequelize db:migrate:undo",
    "generate-model": "sequelize model:generate --name sample --attributes name:string",
    "generate-seed": "sequelize seed:generate --name sample",
    "seed": "sequelize-cli db:seed:all",
    "drop-db": "sequelize db:drop",
    "create-db": "sequelize db:create",
    "dev": "nodemon server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/excalibur-erp/erp-be.git"
  },
  "author": "",
